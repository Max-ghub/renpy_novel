transform dynamic_bagio:
    xoffset -110
    yoffset 100
    parallel:
        ease 2.5 yoffset 140
        ease 2.5 yoffset 50
        repeat
    parallel:
        ease 3 rotate -1.5
        ease 3 rotate 1.5
        repeat
    parallel:
        ease 3 xoffset -125
        ease 3 xoffset -100
        repeat